	</properties>

	<dependencies>
		<dependency>
			<groupId>com.neusoft.hp</groupId>
			<artifactId>mybatis-plugin</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>
	<build>
		<finalName>${project.obj.artifactId}-${project.obj.version}</finalName>
		<plugins>
			<plugin>  
				<groupId>org.codehaus.mojo</groupId>  
				<artifactId>exec-maven-plugin</artifactId>  
				<version>1.6.0</version>  
				<executions>  
					<execution>  
						<id>create-src</id>
						<phase>generate-sources</phase>
						<goals>  
							<goal>exec</goal>  
						</goals>  
						<configuration>
							<executable>CMD</executable>
							<arguments>
								<argument>/C</argument>
								<argument>MKDIR src</argument>
							</arguments>
						</configuration>
					</execution>  
					<execution>  
						<id>create-src-main</id>
						<phase>generate-sources</phase>
						<goals>  
							<goal>exec</goal>  
						</goals>  
						<configuration>
							<executable>CMD</executable>
							<arguments>
								<argument>/C</argument>
								<argument>MKDIR src\main</argument>
							</arguments>
						</configuration>
					</execution>  
					<execution>  
						<id>create-src-main-java</id>
						<phase>generate-sources</phase>
						<goals>  
							<goal>exec</goal>  
						</goals>  
						<configuration>
							<executable>CMD</executable>
							<arguments>
								<argument>/C</argument>
								<argument>MKDIR src\main\java</argument>
							</arguments>
						</configuration>
					</execution>  
					<execution>  
						<id>create-src-main-resources</id>
						<phase>generate-sources</phase>
						<goals>  
							<goal>exec</goal>  
						</goals>  
						<configuration>
							<executable>CMD</executable>
							<arguments>
								<argument>/C</argument>
								<argument>MKDIR src\main\resources</argument>
							</arguments>
						</configuration>
					</execution>  
					<execution>  
						<id>del-src</id>
						<phase>verify</phase>
						<goals>  
							<goal>exec</goal>  
						</goals>  
						<configuration>
							<executable>CMD</executable>
							<arguments>
								<argument>/C</argument>
								<argument>RMDIR /S /Q src</argument>
							</arguments>
						</configuration>
					</execution>  
					<execution>  
						<id>create-install</id>
						<phase>verify</phase>
						<goals>  
							<goal>exec</goal>  
						</goals>  
						<configuration>
							<executable>CMD</executable>
							<arguments>
								<argument>/C</argument>
								<argument>ECHO mvn install:install-file -Dfile=${project.obj.artifactId}-${project.obj.version}.jar -DgroupId=${project.obj.groupId} -DartifactId=${project.obj.artifactId} -Dversion=${project.obj.version} -Dpackaging=jar > target\install.bat</argument>
							</arguments>
						</configuration>
					</execution>  
					<execution>  
						<id>create-deploy</id>
						<phase>verify</phase>
						<goals>  
							<goal>exec</goal>  
						</goals>  
						<configuration>
							<executable>CMD</executable>
							<arguments>
								<argument>/C</argument>
								<argument>ECHO mvn deploy:deploy-file -Dfile=${project.obj.artifactId}-${project.obj.version}.jar -DgroupId=${project.obj.groupId} -DartifactId=${project.obj.artifactId} -Dversion=${project.obj.version} -Dpackaging=jar -Durl=http://192.168.138.101:8081/repository/hees_snapshots/ -DrepositoryId=nexus > target\deploy.bat</argument>
							</arguments>
						</configuration>
					</execution>  					
				</executions>  
			</plugin> 
			<plugin>
				<groupId>com.neusoft.hp</groupId>
				<artifactId>dubbo-deploy</artifactId>
				<version>0.0.1-SNAPSHOT</version>
				<dependencies>
					<dependency>
						<groupId>com.oracle</groupId>
						<artifactId>ojdbc</artifactId>
						<version>10.2.0.3.0</version>
					</dependency>
				</dependencies>
				<executions>
					<execution>
						<id>gen-xml</id>
						<phase>generate-resources</phase>
						<goals>
							<goal>gen</goal>
						</goals>
						<configuration>
							<myDriverClass>oracle.jdbc.driver.OracleDriver</myDriverClass>
							<myConnectionURL>jdbc:oracle:thin:@192.168.138.111:1521:ora11g</myConnectionURL>
							<myUserId>ime_platform</myUserId>
							<myPassword>ime_platform</myPassword>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.mybatis.generator</groupId>
				<artifactId>mybatis-generator-maven-plugin</artifactId>
				<version>1.3.5</version>
				<dependencies>
					<dependency>
						<groupId>com.oracle</groupId>
						<artifactId>ojdbc</artifactId>
						<version>10.2.0.3.0</version>
					</dependency>
					<dependency>
						<groupId>com.neusoft.hp</groupId>
						<artifactId>mybatis-plugin</artifactId>
						<version>${project.version}</version>
					</dependency>
				</dependencies>
				<executions>
					<execution>
						<id>mybatis</id>
						<phase>process-resources</phase>
						<goals>
							<goal>generate</goal>
						</goals>	
						<configuration>
							<!--配置文件的路径 -->
							<configurationFile>${basedir}/src/main/generatorConfig.xml</configurationFile>
							<overwrite>true</overwrite>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>	
				<executions>
					<execution>
						<id>default-compile</id>
						<phase>compile</phase>
						<goals>
							<goal>compile</goal>
						</goals>
						<configuration>
							<encoding>${project.charset}</encoding>
							<compilerVersion>${project.jdk.version}</compilerVersion>

						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>3.0.2</version>
				<executions>
					<execution>
						<id>default-resources</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>resources</goal>
						</goals>
						<configuration>
							<outputDirectory>${project.build.outputDirectory}</outputDirectory>
							<resources>
								<resource>
									<directory>src/main/resources</directory>
								</resource>
							</resources>
						</configuration>
					</execution>
					<execution>
						<id>source-resources</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>copy-resources</goal>
						</goals>
						<configuration>
							<outputDirectory>${basedir}/target</outputDirectory>
							<resources>
								<resource>
									<directory>src/main/java</directory>
								</resource>
							</resources>
						</configuration>
					</execution>					
				</executions>	

			</plugin>
			<plugin>  
				<groupId>org.apache.maven.plugins</groupId>  
				<artifactId>maven-jar-plugin</artifactId>  
				<version>3.0.2</version>  
				<executions>
					<execution>
						<id>default-packaging</id>
						<phase>package</phase>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>				
			</plugin>  
		</plugins>
	</build>
</project>