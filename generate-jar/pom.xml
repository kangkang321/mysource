	<groupId>com.neusoft.hp</groupId>
	<artifactId>generate-jar</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>

	<properties>
		<project.charset>UTF-8</project.charset>
		<project.jdk.version>1.8</project.jdk.version>
